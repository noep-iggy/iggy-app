{
  "expo": {
    "name": "Iggy",
    "slug": "iggy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/app/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "iggy",
    "splash": {
      "image": "./src/assets/images/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#228752"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.noep.iggyapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app/adaptive-icon.png",
        "backgroundColor": "#228752"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.noep.iggyapp"
    },
    "web": {
      "favicon": "./src/assets/images/app/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "86c50c50-136b-4a7a-968f-7ece40ef5a21"
      }
    },
    "owner": "iggy-app"
  }
}
